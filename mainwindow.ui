}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
}

/* QLabel styling */
QLabel {
    font-size: 14px;
    color: #333333;
    padding: 5px;
    border: none; /* Remove border if not needed */
    border-radius: 5px;         /* Add border-radius for rounded corners */
}
