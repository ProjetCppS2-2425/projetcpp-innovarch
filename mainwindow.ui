             <property name="styleSheet">
              <string notr="true">/* QWidget styling */
QWidget {
    border: 2px solid #4e1764;  /* Set border width and color */
    padding: 10px;              /* Set padding inside the widget */
	border-radius: 5px;
}

/* QLineEdit styling */
QLineEdit {
    background-color: #f0f0f0;
    border: 1px solid #5c5c5c;
    border-radius: 5px;         /* Corrected property and added border-radius */
    padding: 5px;
    font-size: 14px;
    color: #333333;
}

QLineEdit:hover {
    border: 1px solid #0078d7;
}

QLineEdit:focus {
    border: 1px solid #3b5998;
    background-color: #ffffff;
}

/* QDateEdit styling */
QDateEdit {
    background-color: #f0f0f0;
    border: 1px solid #5c5c5c;
    padding: 5px;
    font-size: 14px;
    color: #333333;
    border-radius: 5px;         /* Add border-radius for rounded corners */
}

QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid #5c5c5c;
}

/* QComboBox styling */
QComboBox {
    background-color: #f0f0f0;
    border: 1px solid #5c5c5c;
    padding: 5px;
    font-size: 14px;
    color: #333333;
    border-radius: 5px;         /* Add border-radius for rounded corners */
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid #5c5c5c;
}

/* QPushButton styling */
QPushButton {
    color: black;
    border: 1px solid #5c5c5c;
    padding: 5px 15px;
    font-size: 14px;
    border-radius: 5px;         /* Add border-radius for rounded corners */
}

QPushButton:hover {
    background-color: rgb(252, 246, 243);
}

QPushButton:pressed {
    background-color: #C0C0C0; /* Change background color when pressed */
    border: 1px solid black; /* Change border color when pressed */
}

QPushButton:disabled {
    background-color: #C0C0C0;
    color: #a0a0a0;
    border: 1px solid #d3d3d3;
}

/* QCheckBox styling */
QCheckBox {
    spacing: 5px;
    font-size: 14px;
    color: #333333;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
}

/* QLabel styling */
QLabel {
    font-size: 14px;
    color: #333333;
    padding: 5px;
    border: none; /* Remove border if not needed */
    border-radius: 5px;         /* Add border-radius for rounded corners */
}
</string>
