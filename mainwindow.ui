}

/* QDateEdit styling */
QDateEdit {
    background-color: #f0f0f0;
    border: 1px solid #5c5c5c;
    padding: 5px;
    font-size: 14px;
    color: #333333;
    border-radius: 5px;         /* Add border-radius for rounded corners */
}

QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid #5c5c5c;
}

/* QComboBox styling */
QComboBox {
    background-color: #f0f0f0;
    border: 1px solid #5c5c5c;
    padding: 5px;
    font-size: 14px;
    color: #333333;
    border-radius: 5px;         /* Add border-radius for rounded corners */
}

